<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.chendehe.dao.UserDao">
  <select id="getAll" resultType="com.chendehe.entity.UserEntity">
    select * from t_user where id = #{id}
  </select>

  <select id="getOne" resultType="com.chendehe.entity.UserEntity">
    select * from t_user where id = #{id}
  </select>

  <select id="save">
    select * from t_user where id = #{id}
  </select>

  <select id="update">
    select * from t_user where id = #{id}
  </select>

  <select id="delete">
    select * from t_user where id = #{id}
  </select>
</mapper>